import math

list = []
for i in range(0,15): list.append(math.pow(5,i))
print(list)

list2 = []
for i in range(1,21): list2.append(math.factorial(i))
print(list2)

list3 = []
for i in range(1,19): list3.append(math.pow(math.e,i))
print(list3)

nazwiska = ["Kowalski", "Nowak", "Wisniewski"]
list4 = []
for i in nazwiska: list4.append(len(i))
print(list4)
