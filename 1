list = ["a","b","c"]
list.append("d")
print(list)

list2 = [1,2,3,4]
list.extend(list2)
print(list)

list.insert(3,"b")
print(list)

list.remove("a")
print(list)

list.pop()
print(list)

list2.clear()
print(list2)

index = list.index(3)
print(index)

count = list.count("b")
print(count)

list3 = ["i", "w", "e", "j"]
list3.sort()
print("sort:", list3)

list3.reverse()
print(list3)

list4 = list3.copy()
print(list4)
